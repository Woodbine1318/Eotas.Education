{
  "name": "ap-academy",
  "private": true,
  "description": "Online alternative provision of choice for schools, commissioners and pupils",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ignore-path .gitignore",
    "prettier": "prettier . --ignore-path .gitignore",
    "check-format": "yarn prettier --list-different",
    "format": "yarn prettier --write",
    "validate": "yarn check-format && yarn lint && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/hind": "^4.4.5",
    "@fontsource/poppins": "^4.4.5",
    "autoprefixer": "^10.2.6",
    "gatsby": "^3.6.2",
    "gatsby-plugin-feed": "^3.8.0",
    "gatsby-plugin-gatsby-cloud": "^2.6.1",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-offline": "^4.6.0",
    "gatsby-plugin-postcss": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-plugin-sitemap": "^4.4.0",
    "gatsby-source-contentful": "^5.8.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-remark": "^4.5.0",
    "gatsby-transformer-sharp": "^3.6.0",
    "postcss": "^8.3.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.+(js|json|jsx)": [
      "prettier --write",
      "prettier --list-different"
    ],
    "*.(js|jsx)": [
      "eslint"
    ],
    "*.--write": "prettier --write"
  }
}
